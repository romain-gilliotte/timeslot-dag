{
  "name": "timeslot-dag",
  "version": "0.1.4",
  "description": "Helper library to work with periods of time in javascript",
  "main": "dist/timeslot.js",
  "module": "lib/timeslot.js",
  "author": "Romain Gilliotte",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/romain-gilliotte/timeslot.js"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": []
  },
  "esdoc": {
    "source": "./lib",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "test": {
            "source": "./test/",
            "interfaces": [
              "describe",
              "it",
              "context",
              "suite",
              "test"
            ],
            "includes": [
              "\\.js$"
            ],
            "excludes": [
              "\\.config\\.js$"
            ]
          }
        }
      }
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "babel-cli": "x",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-preset-env": "x",
    "chai": "^4.1.2",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "rimraf": "x"
  },
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "./node_modules/.bin/rimraf dist docs",
    "build": "./node_modules/.bin/babel lib -d dist",
    "test": "./node_modules/.bin/mocha --require babel-core/register --require babel-polyfill",
    "gen-doc": "./node_modules/.bin/esdoc"
  }
}
